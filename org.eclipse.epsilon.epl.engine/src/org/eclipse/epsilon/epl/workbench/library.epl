patterns Graph {
	
	pattern A2B
		f : L!t_node,
		trans : L!t_transition 
			in: L!t_transition.all.first() , 
		to : L!t_node 
			in : L!t_node.all.second()
			guard : true {
		
		match : (f.a_name == trans.a_from) and (to.a_name == trans.a_to)
		
		do {
			"Success".println();
		}
		
		nomatch {
			"Failure".println();
		}
		
	}
	
}

operation foo() {}

operation L!t_transition getName() {
	return self.a_from + "->" + self.a_to;
}